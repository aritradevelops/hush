{
  "name": "barebone",
  "scripts": {
    "dev": "nodemon --legacy-watch --files src/index.ts src/queries.sql",
    "test": "tsx src/test.ts",
    "prod": "NODE_ENV=production node dist/index.js",
    "prod:watch": "NODE_ENV=production nodemon --hot dist/index.js",
    "cli": "NODE_ENV=development ts-node -T cli/index.ts",
    "build": "rm -fr dist && tsc --build",
    "d:dev": "docker-compose up",
    "d:dev:down": "docker-compose down",
    "d:cli": "docker exec -it barebone-server npm run cli",
    "d:db": "cd db && docker-compose up -d",
    "d:db:restart": "cd db && docker-compose up --force-recreate",
    "d:db:down": "cd db && docker-compose down",
    "d:db:attach": "docker exec -it postgresql psql -U postgres -d barebone"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@tanstack/react-query": "^5.69.0",
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-custom-errors": "^0.0.5",
    "cookie": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "i18next": "^24.0.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.0",
    "inflection": "^3.0.0",
    "jose": "^5.9.6",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "pg": "^8.13.1",
    "qs": "^6.13.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.0.1",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@inquirer/prompts": "^7.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.34",
    "@types/qs": "^6.9.17",
    "@types/react": "^19.0.2",
    "@types/string-template": "^1.0.6",
    "class-validator-jsonschema": "^5.0.1",
    "commander": "^12.1.0",
    "nodemon": "^3.1.7",
    "pg-minify": "^1.7.0",
    "string-template": "^1.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "yaml": "^2.6.1"
  }
}
